<div class="jumbotron">
	<h2><%= @category.name %></h2>
	<ul>
		<% @category.items.each do |item| %>
		<li>
			<div class="col-md-2">
				<ul>
					<li>
						<%= link_to(image_tag(item.image), item) %>
					</li>
					<li>
						<%= item.name %>
					</li>
				</ul>
			</div>
			
		</li>
		<% end %>
	</ul>
</div>
	
